        </activity>
        <activity
            android:name="com.CFP.messiah.SpeedDialActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.CFP.messiah.ContactSelection"
