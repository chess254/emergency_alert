        <activity
            android:name="com.CFP.messiah.Messiah_Need_Maps"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.CFP.messiah.Settings"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
